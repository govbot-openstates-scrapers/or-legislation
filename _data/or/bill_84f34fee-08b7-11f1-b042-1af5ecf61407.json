{"legislative_session": "2026R1", "identifier": "SB1545", "title": "Relating to corner crossing; declaring an emergency.", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "summary", "abstract": "Establishes immunity from action for trespass for persons that corner cross on public land. Establishes immunity for landowner for damages due to negligence or gross negligence arising out of corner crossing. Excludes lawful corner crossing from the definition of \"enter or remain unlawfully\" for purposes of criminal trespass laws. Declares an emergency, effective on passage."}], "other_titles": [], "other_identifiers": [], "actions": [], "sponsorships": [{"name": "Mike McLane", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Mike McLane\"}", "organization_id": null}, {"name": "Anthony Broadman", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Anthony Broadman\"}", "organization_id": null}, {"name": "Jeff Golden", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Jeff Golden\"}", "organization_id": null}, {"name": "Todd Nash", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Todd Nash\"}", "organization_id": null}, {"name": "David Brock Smith", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"David Brock Smith\"}", "organization_id": null}, {"name": "Ben Bowman", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Ben Bowman\"}", "organization_id": null}, {"name": "Vikki Breese-Iverson", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Vikki Breese-Iverson\"}", "organization_id": null}, {"name": "Matt Bunch", "classification": "cosponsor", "ent